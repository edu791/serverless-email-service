{
  "name": "serverless-mail-service",
  "version": "1.0.0",
  "description": "Serverless mail service",
  "main": "handler.js",
  "private": "false",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "offline": "serverless offline start --stage=dev",
    "deploy:dev": "serverless deploy -v --stage=dev",
    "deploy:prod": "serverless deploy -v --stage=prod",
    "prettier": "prettier --config .prettierrc -l",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.2",
    "@types/ejs": "^3.0.4",
    "aws-sdk": "^2.841.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "ejs": "^3.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "husky": "^5.0.9",
    "prettier": "^2.2.1",
    "serverless-offline": "^6.5.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
